<svg width="{{window.width}}" height="{{window.height}}">
    {{#each outputs}}
    <g class="{{device}}">
        {{#each leds}}
        {{#unless @first}}
        data-to-circle-id="{{index}}" x2="{{x}}" y2="{{y}}" />
        {{/unless}}
        {{#unless @last}}
        <line class="line" id="o{{@../index}}_c{{index}}l" data-from-circle-id="{{index}}" x1="{{x}}" y1="{{y}}"
        {{/unless}}
        {{/each}}
        {{#each leds}}
        {{!-- <line x1="{{x}}" y1="{{y}}" x2="{{x}}" y2="{{y}}" data-radius-id="{{r}}" class="circle" id="o{{@../index}}_c{{index}}" /> --}}
        <line x1="{{x}}" y1="{{y}}" x2="{{x}}" y2="{{y}}" data-radius-id="{{r}}" data-circle-id="{{index}}" class="circle" id="o{{@../index}}_c{{index}}" />
        {{/each}}
    </g>
    {{/each}}
</svg>