<div class="item">
    <p>{{demo.id}}</p>
    <p>{{demo.description}}</p>
    <img src="{{{img_src}}}" />
    {{#each files}}
        {{#each this}}
            <button onclick="buttonClicked('{{../../demo.id}}', '{{@index}}')">Sync</button>
            <h4>{{@key}}</h4>
            <textarea class="{{../../demo.id}} {{@index}}">{{{this}}}</textarea>
        {{/each}}
    {{/each}}
</div>